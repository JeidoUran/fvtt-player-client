{
  "name": "vtt-desktop-client",
  "packageManager": "yarn@1.22.22",
  "version": "1.11.0-dev",
  "license": "MIT",
  "description": "FVTT Desktop Client",
  "main": ".vite/build/main.js",
  "author": "theripper93",
  "contributors": [
